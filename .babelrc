{
  "presets": [
    "@babel/typescript",
    "@babel/react",
    [
      "@babel/env",
      {
        "targets": {
          "node": "8"
        }
      }
    ]
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "babel-plugin-typescript-to-proptypes"
  ]
}
